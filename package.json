{
  "name": "cybersoluce-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "prepare": "node -e \"try { if (process.env.HUSKY !== '0' && !process.env.CI && process.env.NODE_ENV !== 'production') { require('husky').install() } } catch (e) { /* husky not installed, skip */ }\"",
    "audit:fix": "npm audit fix --force",
    "security:check": "npm audit --audit-level moderate",
    "deps:update": "npm update",
    "deps:check": "npm outdated",
    "verify": "node scripts/verify-launch-readiness.js",
    "verify:production": "node scripts/verify-production.js",
    "bundle:check": "node scripts/check-bundle-size.js",
    "pre-launch": "npm run type-check && npm run lint && npm run build && npm run verify",
    "pre-deploy": "npm run type-check && npm run lint && npm run build && npm run verify:production && npm run bundle:check",
    "db:migrate": "node scripts/run-migrations.js",
    "db:migrate:all": "node scripts/run-migrations.js --all",
    "setup:env": "node scripts/create-env-example.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@sentry/react": "^10.19.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.85.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.2.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.5",
    "jose": "^6.0.12",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.6.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-intersection-observer": "^9.16.0",
    "react-is": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.20.1",
    "react-use": "^17.4.0",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.1.2",
    "uuid": "^9.0.1",
    "vite": "^5.0.10",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.14",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.16",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.1",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.10",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
